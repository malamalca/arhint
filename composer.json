{
    "name": "malamalca/arhint",
    "description": "Arhim Intranet App",
    "homepage": "https://www.arhim.si",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "cakephp/authentication": "^2.0",
        "cakephp/authorization": "^2.0",
        "cakephp/cakephp": "^4.3.7",
        "cakephp/migrations": "^3.4",
        "cakephp/plugin-installer": "^1.3",
        "malamalca/lil": "^4.5",
        "mikehaertl/phpwkhtmltopdf": "2.0.*@dev",
        "phpoffice/phpword": "^0.18.2",
        "syncroton/syncroton": "dev-master",
        "tecnickcom/tcpdf": "dev-main",
        "zendframework/zendframework1": "1.12.*",
        "krivart/php-qr-code": "dev-master"
    },
    "require-dev": {
        "cakephp/bake": "^2.6",
        "cakephp/cakephp-codesniffer": "^4.5",
        "cakephp/debug_kit": "^4.5",
        "josegonzalez/dotenv": "^3.2",
        "phpstan/phpstan": "^0.12.3",
        "phpunit/phpunit": "~8.5.0 || ^9.3",
        "vimeo/psalm": "^4.15"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "syncroton/syncroton",
                "source": {
                    "url": "https://github.com/tine20/syncroton.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "vcs",
            "url": "git@github.com:malamalca/Qr-Code.git"
        }
    ],
    "suggest": {
        "markstory/asset_compress": "An asset compression plugin which provides file concatenation and a flexible filter system for preprocessing and minification.",
        "dereuromark/cakephp-ide-helper": "After baking your code, this keeps your annotations in sync with the code evolving from there on for maximum IDE and PHPStan/Psalm compatibility."
    },
    "autoload": {
        "psr-0": {
            "Syncroton": [
                "./vendor/syncroton/syncroton/lib/",
                "./vendor/syncroton/syncroton/tests/"
            ]
        },
        "psr-4": {
            "App\\": "src/",
            "Lil\\": "vendor/malamalca/Lil/src/",
            "Crm\\": "plugins/Crm/src/",
            "Expenses\\": "plugins/Expenses/src/",
            "Documents\\": "plugins/Documents/src/",
            "Projects\\": "plugins/Projects/src/",
            "Tasks\\": "plugins/Tasks/src/",
            "Calendar\\": "plugins/Calendar/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests\\",
            "Cake\\Test\\": "vendor\\cakephp\\cakephp\\tests\\",
            "Lil\\Test\\": "vendor\\malamalca\\Lil\\tests\\",
            "Crm\\Test\\": "plugins\\Crm\\tests\\",
            "Expenses\\Test\\": "plugins\\Expenses\\tests\\",
            "Documents\\Test\\": "plugins\\Documents\\tests\\",
            "Projects\\Test\\": "plugins\\Projects\\tests\\",
            "Tasks\\Test\\": "plugins\\Tasks\\tests\\",
            "Calendar\\Test\\": "plugins\\Calendar\\tests\\"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-update-cmd": "App\\Console\\Installer::postUpdate",
        "post-create-project-cmd": "App\\Console\\Installer::postInstall",
        "check": [
            "@test",
            "@cs-check",
            "@stan"
        ],
        "cs-check": "phpcs --colors -p --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/ plugins/Expenses/src/ plugins/Crm/src/ plugins/Documents/src/ plugins/Projects/src/ plugins/Tasks/src/ plugins/Calendar/src/",
        "cs-fix": "phpcbf --colors --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/ plugins/Expenses/src/ plugins/Crm/src/ plugins/Documents/src/ plugins/Projects/src/ plugins/Tasks/src/ plugins/Calendar/src/",
        "stan": "phpstan analyse -l 5 src/ plugins/Expenses/src/ plugins/Crm/src/ plugins/Documents/src/ plugins/Projects/src/ plugins/Tasks/src/ plugins/Calendar/src",
        "test": "phpunit --colors=always"
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true,
            "cakephp/plugin-installer": true
        }
    },
    "minimum-stability": "dev"
}
